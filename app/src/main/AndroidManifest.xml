<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.order" >

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.order.UserApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MusicApp"
        android:usesCleartextTraffic="true" >
        <activity
            android:name="com.example.order.activity.CommentActivity"
            android:exported="true" />
        <activity
            android:name="com.example.order.activity.ShowMyList"
            android:exported="true" />
        <activity
            android:name="com.example.order.activity.ShowListActivity"
            android:exported="true" />
        <activity
            android:name="com.example.order.activity.ListMakeActivity"
            android:exported="true" />
        <activity
            android:name="com.example.order.activity.ChangeSinger"
            android:exported="true" />
        <activity
            android:name="com.example.order.activity.AdminActivity"
            android:exported="true" />
        <activity
            android:name="com.example.order.activity.ChangeUserInfoActivity"
            android:exported="true" />
        <activity
            android:name="com.example.order.activity.MusicListActivity"
            android:exported="true" />
        <activity
            android:name="com.example.order.activity.UploadActivity"
            android:exported="true" />
        <activity
            android:name="com.example.order.activity.SearchActivity"
            android:exported="true" />
        <activity
            android:name="com.example.order.activity.RegisterActivity"
            android:exported="false" />
        <activity
            android:name="com.example.order.activity.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.example.order.activity.LoginActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>